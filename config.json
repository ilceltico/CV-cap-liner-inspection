{
    "circle_detection":{
        "outer":{
            "method": "hough",
            "parameters":{
                "hough":{
                    "average_best_circles": 3,
                    "radius_computation": "border_distance"
                },
                "least_squares": {
                    "circle_generation": "interpolation"
                }
            }
        },
        "inner": {
            "method": "least_squares",
            "parameters": {
                "hough": {
                    "canny_precision": "precise",
                    "average_best_circles": 2
                },
                "least_squares": {
                    "split_blobs": true,
                    "min_blob_dim": 200,
                    "outliers_elimination_type": "votes",
                    "circle_generation": "interpolation"
                }
            }
        }
    },
